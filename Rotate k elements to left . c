INPUT:6 3
 1 2 3 4 5 7
OUTPUT: 4 5 7 1 2 3

#include<stdio.h>
#include<stdlib.h>


int main(){
    int n,k;
    scanf("%d %d",&n,&k);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    
    for(int i=n-k;i<n;i++){
        printf("%d",a[i]);
    }
    for(int i=0;i<n-k;i++){
        printf("%d",a[i]);
      
        }
}
